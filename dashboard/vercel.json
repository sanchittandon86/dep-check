{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, HEAD, OPTIONS" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "^/(?!.*\\.(js|css|png|jpg|svg|ico|woff|woff2|ttf|json)$).*$",
      "destination": "/index.html"
    }
  ]
}
